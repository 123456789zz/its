set console wru=034
set cpu its
set cpu 1024k
set cpu mpx
set rpa dis
set rpb dis
set tua dis
set fha dis
set dpa dis
set dpb dis
set lpt dis
set cr dis
set dc dis
set dtc dis
set dk dis
at ptr bin/ka10/boot/dskdmp.rim
at mta0 out/pdp10-ka/minsrc.tape
set mta mpx=7
set mta type=b
set ai ena
at ai0 out/pdp10-ka/dc10.0
at ai1 out/pdp10-ka/dc10.1
at ai2 out/pdp10-ka/dc10.2
at ai3 out/pdp10-ka/dc10.3
b ptr
