set cpu idle
set cpu mpx
set rpa dis
set rpb dis
set tua dis
set fha dis
set dpa dis
set dpb dis
set lpt dis
set cr dis
set dc dis
set dtc dis
set dk dis
set pd enabled
set pd on
set dpy enabled
set dk disabled
set stk enabled
set tk enabled
at tk 10000 speed=300
# 10001 reserved for GE bagbiters, hah.
set dpk enabled
at dpk 10002 speed=4800
set mty enabled
